{
  "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentParameters.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "clusterName": {
      "value": "aks-cluster"
    },
    "nodeResourceGroupName": {
      "value": "aks-cluster-infrastructure"
    },
    "dnsPrefix": {
      "value": "aks-cluster-dns"
    },
    "agentCount": {
      "value": 1
    },
    "agentVMSize": {
      "value": "Standard_B2s"
    },
    "kubernetesVersion": {
      "value": "1.20.7"
    },
    "networkPlugin": {
      "value": "kubenet"
    },
    "enableRBAC": {
      "value": true
    },
    "enablePrivateCluster": {
      "value": false
    },
    "enableHttpApplicationRouting": {
      "value": false
    }
  }
}
