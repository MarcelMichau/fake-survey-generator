{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ReverseProxy": {
    "Routes": {
      "api": {
        "ClusterId": "api",
        "Match": {
          "Path": "/api/{**catch-all}"
        },
        "Transforms": [
          {
            "PathRemovePrefix": "/api"
          }
        ]
      },
      "ui": {
        "ClusterId": "ui",
        "Match": {
          "Path": "/{**catch-all}"
        }
      }
    },
    "Clusters": {
      "api": {
        "Destinations": {
          "api": {
            "Address": "http://fakesurveygeneratorapi"
          }
        }
      },
      "ui": {
        "Destinations": {
          "api": {
            "Address": "https://localhost:3000"
          }
        }
      }
    }
  }
}
